<?xml version="1.0" encoding="utf-8" ?>
<RecordDescriptionSet Name="DPOAE Screening"
                 Identifier="PM"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <RecordDescriptions>
    
    <!-- DPOAE -->
    <RecordDescription Identifier="DPOAE.TestInformation">
      <ColumnDescription Identifier="Id" Datatype="Guid"/>
      <ColumnDescription Identifier="PatientId" Datatype="Guid"/>
      <ColumnDescription Identifier="UserAccountId" Datatype="Guid"/>
      <ColumnDescription Identifier="LocationId" Datatype="Guid"/>
      <ColumnDescription Identifier="FacilityId" Datatype="Guid"/>
      <ColumnDescription Identifier="TestName" Datatype="String"/>
      <ColumnDescription Identifier="FirmwareVersion" Datatype="UInt32"/>
      <ColumnDescription Identifier="TestTimeStamp" Datatype="DateTime"/>
      <ColumnDescription Identifier="Duration" Datatype="UInt16"/>
      <ColumnDescription Identifier="Ear" Datatype="UInt16"/>
      <ColumnDescription Identifier="TestResult" Datatype="UInt16"/>
      <ColumnDescription Identifier="ProbeType" Datatype="UInt16"/>
      <ColumnDescription Identifier="ProbeSerialNumber" Datatype="UInt32"/>
      <ColumnDescription Identifier="ProbeCounter" Datatype="UInt16"/>
      <ColumnDescription Identifier="InstrumentSerialNumber" Datatype="UInt32"/>
      <ColumnDescription Identifier="Version" Datatype="UInt16"/>
      <ColumnDescription Identifier="ProbeCalibrationDate" Datatype="Date"/>
      <ColumnDescription Identifier="NumberOfTests" Datatype="UInt16"/>
      <ColumnDescription Identifier="FrequencySamplingRate" Datatype="UInt16"/>
      <ColumnDescription Identifier="Frequency1List" IsArray="True" Datatype="UInt16"/>
      <ColumnDescription Identifier="Frequency2List" IsArray="True" Datatype="UInt16"/>
      <ColumnDescription Identifier="NormalizedFrequency1List" IsArray="True" Datatype="UInt16"/>
      <ColumnDescription Identifier="NormalizedFrequency2List" IsArray="True" Datatype="UInt16"/>
      <ColumnDescription Identifier="SingleResultList" IsArray="True" Datatype="UInt16"/>
      <ColumnDescription Identifier="DPOAEList" IsArray="True" Datatype="Float"/>
      <ColumnDescription Identifier="NoiseList" IsArray="True" Datatype="Float"/>
      <ColumnDescription Identifier="FrameCounterList" IsArray="True" Datatype="UInt16"/>
      <ColumnDescription Identifier="ReList" IsArray="True" Datatype="Float"/>
      <ColumnDescription Identifier="ImList" IsArray="True" Datatype="Float"/>
      <ColumnDescription Identifier="EnergyList" IsArray="True" Datatype="Float"/>
      <ColumnDescription Identifier="L1" Datatype="UInt16"/>
      <ColumnDescription Identifier="L2" Datatype="UInt16"/>

      <ColumnDescription Identifier="CalibrationFrames" Datatype="UInt16"/>
      <ColumnDescription Identifier="CalibrationValues" Datatype="UInt16"/>
      <ColumnDescription Identifier="CalibrationGraphScale" Datatype="Float"/>
      <ColumnDescription Identifier="CalibrationGraph" IsArray="True" Datatype="Int8"/>
    
    </RecordDescription>
    
    <!-- A DPOAE Preset -->
    <RecordDescription Identifier="DpoaePreset">
      <ColumnDescription Identifier="Id" Datatype="Guid"/>
      <ColumnDescription Identifier="Name" Datatype="String"/>
      <ColumnDescription Identifier="Category" Datatype="UInt16"/>
      <ColumnDescription Identifier="ConfigName" Datatype="String"/>
      <ColumnDescription Identifier="Category" Datatype="UInt16"/>
      <ColumnDescription Identifier="L1" Datatype="UInt16"/>
      <ColumnDescription Identifier="L2" Datatype="UInt16"/>
      <ColumnDescription Identifier="Frequencies" IsArray="True" Datatype="UInt16"/>
      <ColumnDescription Identifier="FrequencyOrder" IsArray="True" Datatype="UInt16"/>
      <ColumnDescription Identifier="Criterion" Datatype="UInt16"/>
    </RecordDescription>    

  </RecordDescriptions>
</RecordDescriptionSet>